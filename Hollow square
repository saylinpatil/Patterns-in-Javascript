let n = 10;

for(let i=1; i<=n; i++) {
    let output = "";
    for(let j=1; j<=n; j++) {
        if(j == 1 || j == n || i == 1 || i == n) {
        output += "* ";
       } else {
           output += "  ";
       }
    }
    console.log(output);
}

Output -

* * * * * * * * * * 
*                 * 
*                 * 
*                 * 
*                 * 
*                 * 
*                 * 
*                 * 
*                 * 
* * * * * * * * * * 
