let n=9;
for(let i=1;i<=n;i++){
  output="";
  for(let j=1;j<=n;j++){
    if(i==j||i+j==n+1||i==n||i==1){
    output+="* ";
    }else{
       output+="  ";
    }
  }
  console.log(output);
}


