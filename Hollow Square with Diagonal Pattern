let n=9;
for(let i=1;i<=n;i++){
    output="";
    for(let j=1;j<=n;j++){
        if(i==1||i==n||j==1||j==n||j==i||j+i==n+1){
        output+="* ";
        }else{
            output+="  ";
        }
    }
    console.log(output);
}


